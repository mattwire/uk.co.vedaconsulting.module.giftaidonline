    <url desc="Main Extension Page">http://civicrm.org/extensions/uk-hmrc-online-gift-aid-submission</url>
    <url desc="Support">https://mjw.pt/support/giftaidonline</url>
    <url desc="Release Notes">https://github.com/mattwire/uk.co.vedaconsulting.module.giftaidonline/blob/mjwconsulting/docs/release/release_notes.md</url>
    <url desc="Licensing">https://civicrm.org/licensing</url>
  </urls>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Matthew Wire (MJW Consulting), Parvez Saleh</author>
    <email>mjw@mjwconsult.co.uk, support@vedaconsulting.co.uk</email>
  </maintainer>
  <releaseDate>2020-04-27</releaseDate>
  <version>1.8.3</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.24</ver>
  </compatibility>
  <comments>Production Release, please confirm the receipt of the submission with the HMRC</comments>
